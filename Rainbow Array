#include <iostream>

using namespace std;

string rainbow[7];
int commandLineInt;

int main(int argc, char* argv[])
{
	string rainbow[7] = { "Red", "Yellow", "Pink", "Green", "Orange", "Purple", "Blue" };
	if (argc > 1)
	{
    commandLineInt = atoi(argv[1]);
		cout << rainbow[commandLineInt -1] << endl;
	}

	return 0;
}
